{
  "name": "@hunteroi/discord-mailbox",
  "version": "3.0.0",
  "description": "A framework to integrate a mailbox inside your Discord bot built with DiscordJS",
  "main": "lib/index.js",
  "scripts": {
    "start": "yarn build & node ./examples/index.js",
    "build": "npx tsc",
    "build:docs": "typedoc",
    "prepublish": "yarn build:docs",
    "format": "npx prettier --write \"src/**/*.ts\" \"examples/**/*.js\" ",
    "lint": "npx tslint -p tsconfig.json"
  },
  "repository": "git://github.com/hunteroi/discord-mailbox.git",
  "keywords": [
    "mailbox",
    "discordjs",
    "modmail"
  ],
  "author": "HunteRoi <me@tinaeldevresse.eu> (https://tinaeldevresse.eu/)",
  "funding": "https://github.com/sponsors/hunteroi",
  "engines": {
    "node": ">=16.6.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hunteroi/discord-mailbox/issues"
  },
  "homepage": "https://github.com/hunteroi/discord-mailbox#readme",
  "dependencies": {
    "cron": "^2.1.0",
    "discord.js": "^14.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cron": "^2.0.0",
    "@types/luxon": "^2.4.0",
    "@types/node": "^17.0.45",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.21",
    "typescript": "^4.7.4"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*"
  ]
}
