{
	"name": "@hunteroi/discord-mailbox",
	"version": "2.1.0",
	"description": "A framework to integrate a mailbox inside your Discord bot built with DiscordJS",
	"main": "lib/index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" --tab-width 4",
		"lint": "tslint -p tsconfig.json",
		"prepare": "npm run build",
		"example": "npm run build & node ./example/index.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hunteroi/discord-mailbox.git"
	},
	"keywords": [
		"mailbox",
		"discordjs",
		"modmail"
	],
	"author": "HunteRoi",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/hunteroi/discord-mailbox/issues"
	},
	"homepage": "https://github.com/hunteroi/discord-mailbox#readme",
	"dependencies": {
		"cron": "^1.8.2",
		"discord.js": "^13.2.0",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@types/cron": "^1.7.3",
		"@types/moment": "^2.13.0",
		"@types/uuid": "^8.3.1",
		"@types/node": "^16.11.0",
		"@types/ws": "^8.2.0",
		"tslint": "^6.1.0",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^4.4.4"
	},
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib/**/*"
	]
